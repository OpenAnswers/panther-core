{
  "name": "oa-event_monitors",
  "description": "OpenAnswers Event Monitors",
  "version": "4.0.3",
  "private": true,
  "author": "OpenAnswers <support+panther@openanswers.co.uk>",
  "homepage": "http://openanswers.co.uk/",
  "scripts": {
    "auditx": "npm audit --registry=https://registry.npmjs.org",
    "start": "yarn run http",
    "test": "npm run unit",
    "unit": "mocha test/unit/**/*_spec.coffee",
    "func": "mocha test/func/**/*_spec.coffee",
    "syslogd": "node bin/oamon.js --name syslogd",
    "sample": "node bin/oamon.js --name sample",
    "http": "node bin/oamon.js --name http",
    "pack": "cd ./pkg && npm pack ../",
    "nexus": "npm test && git=$(git status --porcelain) && [ -z \"$git\" ] && npm version patch && npm publish && git push && git push --tags",
    "nexus-testing": "npm test && npm version prerelease && git push && git push --tags && npm publish --tag testing",
    "tag_current": "version=$(node -e 'console.log(require(\"./package.json\").version)') && git tag v${version} && git push --tags"
  },
  "dependencies": {
    "ajv": "^6.12.2",
    "ajv-errors": "^1.0.1",
    "async": "^0.9.0",
    "bluebird": "~3.0.6",
    "body-parser": "^1.14.1",
    "carrier": "0.3.0",
    "debug": "^2.2.0",
    "eventemitter2": "~0.4.14",
    "express": "~4.17.1",
    "glossy": "~0.1.7",
    "imap": "~0.8.14",
    "iniparser": "1.0.3",
    "joose": "~3.50.0",
    "js-yaml": "~3.3.1",
    "libxmljs": "~0.19.0",
    "lodash": "~4.17.15",
    "minimist": "^1.2.5",
    "nconf": "~0.7.1",
    "needle": "~0.11.0",
    "oa-errors": "^4.0.3",
    "oa-event-rules": "^4.0.3",
    "oa-logging": "^4.0.3",
    "oa-tinycache": "^4.0.3",
    "passport": "~0.3.2",
    "socket.io-client": "1.7.4",
    "tailfd": "~0.1.6",
    "toolkit": "~1.5.4",
    "underscore": "~1.8.3"
  },
  "keywords": [
    "oamon",
    "monitoring",
    "event",
    "console",
    "network",
    "management",
    "alert"
  ],
  "engines": {
    "node": ">= 4.2.0"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "chai": "^3.4.0",
    "coffee-script": "^1.10.0",
    "eslint": "^4.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-lodash": "^2.4.5",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "mocha": "^2.3.3",
    "moment": "~2.10.6",
    "nock": "^3.3.2",
    "raml-parser": "^0.8.12",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0"
  }
}
