{
  "name": "oa-logging",
  "version": "4.1.0",
  "private": true,
  "description": "OpenAnswers Logging setup",
  "main": "lib/logging.js",
  "engines": {
    "node": ">=16.13.0 <17"
  },
  "files": [
    "lib/*.js"
  ],
  "scripts": {
    "test": "npm run compile && mocha test/*_spec.coffee",
    "build": "rimraf lib && npm run compile",
    "compile": "coffee -co lib/ src/",
    "pack": "npm run build && cd pkg && npm pack ../",
    "check_git_status": "git=$(git status --porcelain) && [ -z \"$git\" ]"
  },
  "keywords": [
    "logging",
    "log",
    "openanswers",
    "oa",
    "debug"
  ],
  "author": "OpenAnswers <support+panther@openanswers.co.uk>",
  "license": "CPAL-1.0",
  "dependencies": {
    "debug": "4.3.4",
    "oa-helpers": "^4.1.0",
    "winston": "2.4.6"
  },
  "devDependencies": {
    "chai": "^3.1.0",
    "coffeescript": "^1.10.0",
    "mocha": "~9.2.2",
    "rimraf": "^3.0.2",
    "sinon": "^1.14.1",
    "winston-spy": "^0.2.0"
  }
}
