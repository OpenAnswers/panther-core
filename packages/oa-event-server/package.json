{
  "name": "oa-event-server",
  "description": "OpenAnswers Event Server",
  "version": "4.0.3",
  "private": true,
  "author": "OpenAnswers <support+panther@openanswers.co.uk>",
  "homepage": "http://openanswers.co.uk/",
  "bin": {
    "oamon_server": "./server.sh"
  },
  "scripts": {
    "auditx": "npm audit --registry=https://registry.npmjs.org",
    "start": "node lib/server.js",
    "test": "npm run unit",
    "unit": "mocha test/unit",
    "casper": "casperjs test test/casperjs",
    "mochacasper": "mocha-casperjs test/mocha-casperjs/*.coffee",
    "pack": "cd ./pkg && npm pack ../",
    "nexus": "npm test && git=$(git status --porcelain) && [ -z \"$git\" ] && npm version patch && npm publish && git push && git push --tags",
    "nexus-testing": "npm test && npm version prerelease && git push && git push --tags && npm publish --tag testing",
    "tag_current": "version=$(node -e 'console.log(require(\"./package.json\").version)') && git tag v${version} && git push --tags"
  },
  "dependencies": {
    "ajv": "^5.3.0",
    "async": "^1.5",
    "bluebird": "~3.0.6",
    "body-parser": "^1.17.2",
    "connect": "~3.0",
    "cookie-parser": "^1.4.5",
    "ejs": "^2.3",
    "eventemitter2": "~0.4",
    "express": "^4.17.1",
    "express-ejs-layouts": "^1.1.0",
    "express-session": "^1.17.1",
    "hooks": "^0.3.2",
    "joose": "^3.50.0",
    "lodash": "~4.17.15",
    "minimist": "^1.2.5",
    "mongodb": "2.2.36",
    "mongoose": "4.13.19",
    "multiparty": "^4.1.1",
    "nconf": "^0.7.1",
    "needle": "^1.6.0",
    "oa-errors": "^4.0.3",
    "oa-event-rules": "^4.0.3",
    "oa-logging": "^4.0.3",
    "opts": "^1.2.2",
    "session-mongoose": "^0.5.2",
    "socket.io": "1.7.4",
    "socket.io-client": "1.7.4",
    "underscore": "~1.8.2"
  },
  "keywords": [
    "mongodb",
    "event",
    "console",
    "network",
    "management",
    "alert",
    "panther"
  ],
  "engines": {
    "node": ">= 4.0.0"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/bluebird": "^3.5.4",
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/express-session": "0.0.32",
    "@types/lodash": "^4.14.64",
    "@types/mongodb": "^2.2.2",
    "@types/mongoose": "^4.7.13",
    "@types/socket.io": "^1.4.29",
    "@types/socket.io-client": "^1.4.33",
    "chai": "^1.10.0",
    "coffee-script": "^1.10.0",
    "eslint": "^4.13.1",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-lodash": "^2.5.0",
    "eslint-plugin-node": "^4.2.3",
    "eslint-plugin-promise": "^3.5.0",
    "grunt": "^0.4.5",
    "mocha": "^2.3.3",
    "request": "~2.88",
    "sinon": "^1.17.2",
    "tough-cookie": "^0.12.1",
    "typescript": "^2.3.4",
    "xmldom": "^0.1.19"
  }
}
