{
  "name": "oa-event-console",
  "version": "4.1.3",
  "private": true,
  "description": "Event Console, like a panther",
  "main": "app.js",
  "engines": {
    "node": ">=16.13.0 <19"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist.js",
    "install-deps": "grunt install-deps",
    "build": "rimraf dist && grunt build-dist",
    "setup": "yarn run setup/severity && yarn run setup/default-admin",
    "setup/severity": "node dist/bin/setup_severity -c config.yml",
    "setup/default-admin": "node dist/bin/setup_user -c config.yml --user admin --group admin --password admin --email admin@example.com",
    "compile": "grunt coffee",
    "coffeecompile": "grunt coffee build-dev",
    "test": "npm run unit && npm run int",
    "test-all": "mocha $NPM_MOCHA_OPTS test/**/*_spec.coffee",
    "unit": "mocha $NPM_MOCHA_OPTS test/unit/**/*_spec.coffee",
    "int": "mocha $NPM_MOCHA_OPTS test/int/**/*_spec.coffee",
    "func": "mocha $NPM_MOCHA_OPTS test/func/**/*_spec.coffee",
    "load": "mocha $NPM_MOCHA_OPTS test/load/**/*_spec.coffee",
    "webdriver": "mocha $NPM_MOCHA_OPTS --bail test/webdriver/**/*_spec.coffee",
    "webdriver-chrome": "BROWSER=chrome npm run webdriver",
    "webdriver-firefox": "BROWSER=firefox npm run webdriver",
    "webdriver-ie": "vagrant up && BROWSER_HOST=192.168.60.10 BROWSER_PORT=4444 BROWSER=\"internet explorer\" npm run webdriver",
    "webdriver-selenium": "java -jar pkg/selenium-server-standalone-2.52.0.jar",
    "webdriver-get-selenium": "wget http://selenium-release.storage.googleapis.com/2.52/selenium-server-standalone-2.52.0.jar -O pkg/selenium-server-standalone-2.52.0.jar",
    "webdriver-phantom": "phantomjs --webdriver=9514 --debug=true --webdriver-loglevel=DEBUG",
    "build:swagger:development": "swagger-jsdoc -o swagger.development.json -d ./swaggerDevelopment.js dist/app/route/*.js dist/app/route/api/*.js dist/app/route/api/event/*.js",
    "build:swagger:production": "swagger-jsdoc -o swagger.production.json -d ./swaggerProduction.js dist/app/route/*.js dist/app/route/api/*.js dist/app/route/api/event/*.js",
    "app": "node app.js",
    "app-test": "node app.js --config test/fixture/config.test2.yml",
    "app_debug": "node --debug-brk app.js",
    "app_gc": "node --trace_gc --trace_gc_verbose app.js",
    "app_coffee": "coffee app/index.coffee",
    "assets": "rm -rf public/assets/bld/ && connect-assets -i app/assets -c js/* css/* bower/* vendor/* -o public/assets/bld -s /assets/bld && cp public/assets/bld/manifest.json builtAssets/",
    "pack": "cd ./pkg && npm run test && npm run assets && npm pack ../",
    "nexus": "npm run assets && npm test && git=$(git status --porcelain) && [ -z \"$git\" ] && npm version patch && npm publish && git push && git push --tags",
    "setup-activity": "bin/setup_activity.coffee",
    "setup-severity": "bin/setup_severity.coffee",
    "setup-user": "bin/setup_user.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.openans.co.uk/Panther/panther-core"
  },
  "keywords": [
    "event",
    "alert",
    "web",
    "console"
  ],
  "author": "OpenAnswers <support+panther@openanswers.co.uk>",
  "license": "CPAL-1.0",
  "homepage": "https://openanswers.co.uk",
  "dependencies": {
    "@hapi/joi": "^16.1.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.19.0",
    "coffee-script": "^1.12.7",
    "coffeescript": "^1.12.7",
    "color": "4.2.3",
    "connect-assets": "~6.0.1",
    "connect-mongo": "4.6.0",
    "cookie-parser": "~1.4.0",
    "debug": "4.3.4",
    "del": "^5.1.0",
    "eventemitter2": "~0.4.14",
    "express": "~4.17.1",
    "express-list-endpoints": "^4.0.1",
    "express-session": "1.17.3",
    "feedparser": "~1.1.4",
    "fs-extra": "^7.0.0",
    "glob": "~7.0.0",
    "js-yaml": "3.14.1",
    "jstransformer-markdown-it": "3.0.0",
    "less": "~2.5.0",
    "lodash": "4.17.21",
    "marked": "4.0.18",
    "minimist": "1.2.8",
    "mkdirp": "1.0.4",
    "moment": "2.29.4",
    "mongoose": "=5.13.20",
    "needle": "3.1.0",
    "node-uuid": "~1.4.7",
    "nodemailer": "6.7.7",
    "oa-errors": "^4.1.3",
    "oa-event-rules": "^4.1.3",
    "oa-helpers": "^4.1.3",
    "oa-logging": "^4.1.3",
    "oa-tinycache": "^4.1.3",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "5.0.1",
    "passport.socketio": "3.6.1",
    "pug": "3.0.2",
    "sanitize-filename": "^1.6.1",
    "serve-favicon": "~2.5.0",
    "socket.io": "4.7.1",
    "socketio-file-upload": "^0.7.3",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.4",
    "@types/body-parser": "^1.16.3",
    "@types/eventemitter2": "^4.1.0",
    "@types/express": "^4.11.1",
    "@types/express-session": "0.0.32",
    "@types/less": "0.0.32",
    "@types/lodash": "^4.14.181",
    "@types/minimist": "^1.2.0",
    "@types/moment": "^2.13.0",
    "@types/node-uuid": "0.0.28",
    "@types/passport": "^0.3.3",
    "bower": "1.8.14",
    "chai": "^3.4.1",
    "coffeelint": "^1.16.0",
    "eslint": "8.19.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-lodash": "^2.4.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "grunt": "1.5.3",
    "grunt-bowercopy": "^1.2.0",
    "grunt-cli": "1.4.3",
    "grunt-coffeelint": "0.0.13",
    "grunt-contrib-coffee": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "1.1.0",
    "grunt-exec": "^0.4.6",
    "grunt-mocha-test": "^0.12.7",
    "grunt-zip": "^0.17.1",
    "jshint": "^2.9.4",
    "mocha": "~9.2.2",
    "nock": "13.2.8",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sinon": "^1.14.1",
    "socket.io-client": "4.7.1",
    "supertest": "6.3.3",
    "tough-cookie": "4.1.3",
    "webdriverio": "7.20.5",
    "winston-spy": "^0.2.0"
  }
}
