script(id="template-select-field-view" type="text/x-mustache-template")
  .select-entry-view(data-select-id="{{data.verb}}")
    .inner
      span.select-view-text The field&nbsp;
      span.select-view-text.select-field-view #[code {{ data.field }}]
      span.select-view-text  {{data.friendly_before}}
      span.select-view-text.select-operator-view {{ data.verb_english }}
      span.select-view-text  {{data.friendly_after}}
