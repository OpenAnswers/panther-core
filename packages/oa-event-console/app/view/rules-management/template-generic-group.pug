script(id="template-generic-group" type="text/x-mustache-template")
  .card.card-generic.margintop15(data-group="{{ data.name }}" class="{{ data.class }}")
    .inner
      .title
        .right
          .generic-grouping-icon
            span.glyphicon.glyphicon-menu-hamburger.generic-group-menu.hidden
            span.glyphicon.glyphicon-triangle-top.generic-group-collapse-toggle.hidden
            span.glyphicon.glyphicon-plus.generic-group-add.hidden
            span.glyphicon.glyphicon-question-sign.hidden

        .left
          .generic-grouping-name {{ data.label }} 
          .generic-grouping-subtitle {{ data.label_detail }} 
      .content.margintop10
        #generic-grouping-replace-me
        ul#ruleset-grouping-replace-me

